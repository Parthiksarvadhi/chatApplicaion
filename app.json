{
  "expo": {
    "name": "Gossip",
    "slug": "chatapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gossip-log.png",
    "scheme": "chatapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "We need camera access to take photos",
        "NSPhotoLibraryUsageDescription": "We need photo library access to send images",
        "NSPhotoLibraryAddUsageDescription": "We need permission to save photos"
      },
      "bundleIdentifier": "com.parthiksalat.gossip"
    },
    "android": {
      "package": "com.parthiksalat.gossip",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/gossip-log.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/gossip-log.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/gossip-log.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "01c28a87-4c01-437e-9c92-6fe1caaf4a8d"
      }
    },
    "owner": "parthiksalat"
  }
}
